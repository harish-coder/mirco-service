package com.example.poc.employee.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.example.poc.employee.dao.EmployeeDao;
import com.example.poc.employee.projection.EmployeeDto;

@Service
public class EmployeeService {

	
	@Autowired
	private EmployeeDao employeeDao;
	
	public List<EmployeeDto> getAllEmpDetails(){
		return employeeDao.getAllEmpDetails();
	}
}
